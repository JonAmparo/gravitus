{
  "name": "gravitus",
  "version": "1.0.0",
  "description": "Gym application called Gravitus. Inspired by the mobile application named Gravitus. I'm trying to replicate the application, while I'm at it. I'm using it as a tool to further my understanding of React, Node.js, Webpack, Babel, MongoDB, Express, and Redux!",
  "author": "Jonathan Amparo",
  "license": "ISC",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "webpack-dev-server --config webpack.dev.js",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "build": "webpack --config production --config webpack.prod.js",
    "heroku-postbuild": "webpack -p --config ./webpack.prod.js --progress"
  },
